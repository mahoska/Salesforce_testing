    <validationRules>
        <fullName>AreaCheck</fullName>
        <active>true</active>
        <errorConditionFormula>Total_area__c &lt;=0  ||  Living_space__c &lt;=0</errorConditionFormula>
        <errorMessage>check the value of the area</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CostCheck</fullName>
        <active>true</active>
        <errorConditionFormula>Minimum_cost__c &lt;0 ||  Initial_cost__c &lt;0</errorConditionFormula>
        <errorMessage>the cost must be a positive number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DateRule</fullName>
        <active>true</active>
        <errorConditionFormula>DATETIMEVALUE(Application_date__c )   &gt;  NOW()</errorConditionFormula>
        <errorDisplayField>Application_date__c</errorDisplayField>
        <errorMessage>date has not yet come</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FloorCheck</fullName>
        <active>true</active>
        <errorConditionFormula>Floor__c &lt; 0 ||  Floor__c &gt;  Number_of_floors_in_the_house__c ||  Number_of_floors_in_the_house__c &lt; 0</errorConditionFormula>
        <errorMessage>specify the number of storeys correctly</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MinCostRule</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Minimum_cost__c &gt;  Initial_cost__c , true, false)</errorConditionFormula>
        <errorDisplayField>Minimum_cost__c</errorDisplayField>
        <errorMessage>Minimum cost should not exceed Initial cost</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NumberRoom</fullName>
        <active>true</active>
        <errorConditionFormula>Number_of_bathrooms__c &lt; 0 ||  Number_of_rooms__c &lt;0</errorConditionFormula>
        <errorMessage>Number of bathrooms or rooms must must be a positive number</errorMessage>
    </validationRules>
